<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.compilation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demos.onSensorchanged.OnSensorChanged" />
        <activity android:name=".demos.login.Login" />
        <activity android:name=".demos.crossReference.CrossReference" />
        <activity android:name=".demos.downloadImageTask.DownloadImageTask" />
        <activity android:name=".demos.doubleTapheart.DoubleTapHeart" />
        <activity android:name=".demos.adapterToggle.AdapterToggle"></activity>
    </application>

</manifest>